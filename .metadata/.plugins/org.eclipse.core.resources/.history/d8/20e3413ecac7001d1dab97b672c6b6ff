package day07.day07;

public class Student {

	private int id;
	private String name;
	private int marks1, marks2, marks3;
	public static int idIncrementor = 101;
	
	
	public Student(){
		id = idIncrementor++;
	}
	public Student(int id, String name, int marks1, int marks2, int marks3) {
		this();
		this.id = id;
		this.name = name;
		this.marks1 = marks1;
		this.marks2 = marks2;
		this.marks3 = marks3;
	}
	
	

}
