
public class TOAProgram01 {

	public static void main(String[] args) {
		int row = 4, col = 5;
		createBoxPattern(row, col);

	}
	
	
	public static void createBoxPattern(int row, int col){
		for(int i = 0; i < row; i ++){
			for(int j = 1; j < col; j++){
				if(i == 0 && j ==0 ){
					System.out.print("*");
				}
				if(i == row-1 && j == col-1){
					System.out.print("*");
				}
			}
		}
	}

}
