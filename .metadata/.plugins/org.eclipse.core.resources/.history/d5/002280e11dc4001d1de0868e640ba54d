package day05;

public class Pp {

	public static void main(String[] args) {
		
		int[] arr = {2,4,5,2,3,2,4,3};
		
		int count = 0;
		int temp = 0;
		for(int i = 0; i <= arr.length-1; i++){
			temp = arr[i];
			for(int j = 1; j <= arr.length-1; j++){
				if(arr[j] == temp){
					count = count + 1;
				}
			}
			System.out.println("Element " + temp + " occurs " + count +" times");
		}

	}

}
