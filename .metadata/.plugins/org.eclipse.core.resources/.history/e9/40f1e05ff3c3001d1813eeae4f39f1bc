
public class TOAProgram01 {

	public static void main(String[] args) {
		int row = 4, col = 5;
		createBoxPattern(row, col);

	}
	
	
	public static void createBoxPattern(int row, int col){
		for(int i = 0; i < row; i ++){
			for(int j = 0; j < col; j++){
				if(i == 1 || i == row || j == 1 || j == col ){
					System.out.print("*");
				}else{
					
					System.out.print(" ");
				}

			}
		}
	}

}
