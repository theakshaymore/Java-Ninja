package day06;

public class Pp02 {

	public static void main(String[] args) {
		int stdId = 64;
		int count = 0;
		while(stdId > 0){
			int rem = stdId % 10;
			stdId = stdId / 10;
			count = rem + count;
		}
		
		if(count < 1 || count > 26){
			System.out.println("not valid");
		}else if(count < 26){
			System.out.println((char)(count+ 64));
		}
		
	}

}
