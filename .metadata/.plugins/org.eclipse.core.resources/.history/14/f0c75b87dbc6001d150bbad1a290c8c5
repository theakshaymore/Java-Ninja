package day06;

public class Pp {

	public static void main(String[] args) {
		
		int range = 10;
		
		System.out.println(getFib(range));

	}
	
	
	public static int getFib(int range){
		for(int i = 0; i<=range; i++){
			
		if(range == 0)
			return 0;
		if(range == 1)
			return 1;
		else
			return getFib(range-1) + getFib(range-2);
		}
	}

}
