package day07.day07;

public class Result {
	
	public static void main(String[] args) {
		Student student = new Student("akshay", 50,30,40);
		resultCalculator(student);
		
	}
	public static void resultCalculator(Student student){
		
		
		System.out.println(student.toString());
		
		if(student.getMarks1() < 40){
			System.out.println("Fail");
		}else{
			int percentage = (student.getMarks1() + student.getMarks2() + student.getMarks3()) / 3;
			if(percentage > 70){
				System.out.println("A");
			}
			if(percentage => 55 && percentage <= 70){
				System.out.println("B");
			}
			if
		}
		
	}
	

}
