package day06;

public class GAProgram02 {

	public static void main(String[] args) {
		for (int i=1; i <= 5; i++) {
            // inner loop for column
            for(int j=1; j <= i; j++) {
                // print star
                System.out.print("* ");
            }
            // new line
            System.out.println();
        }

	}

}
