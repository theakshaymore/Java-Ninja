package day05;

public class Self01 {

	public static void main(String[] args) {
		
		
		String s = "{{}}";
		
		
		checkBraces(s);

	}

	public static boolean checkBraces(String s) {
		int countforOpeningBraces = 0;
		int countforClosingBraces = 0;
		
		for(int i = 0; i < s.length(); i++){
			char c = s.charAt(i);
			if(c == '{'){
				countforOpeningBraces = countforOpeningBraces + 1;
			}
			if(c == '}'){
				countforClosingBraces = countforClosingBraces + 1;
			}

		}
		
		if(countforOpeningBraces == countforClosingBraces){
//			System.out.print("Successfull");
			return true;
		}else{
//			System.out.print("Compilation Error");
			return false;
		}
		
		
	
		
	}

}
