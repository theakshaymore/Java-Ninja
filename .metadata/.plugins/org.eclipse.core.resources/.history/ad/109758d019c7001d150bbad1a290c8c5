package day06;

public class THAProgram02 {

	public static void main(String[] args) {
		
		
		double num = 125.520;
		
		concateSumOfDigits(num);

	}
	
	
	public static void concateSumOfDigits(double num){
		int sumOfLeft = 0, sumOfRight = 0;
		for(int i = 0; i < num; i++){
			double rem = num % 10;
//			System.out.println(rem);
			sumOfLeft = sumOfLeft + rem;
			num = num /10;
		}
		System.out.println(sumOfLeft);
		
	}
	

}
